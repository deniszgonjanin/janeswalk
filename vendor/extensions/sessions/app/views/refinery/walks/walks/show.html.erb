<section class="span10" >
  
  <% content_for :body_content_title do %>
    <p></p>
  <% end %>

    <section>
      <h2><%=raw @walk.title %></h2>
    </section>
    <section>
      <h3 class="bottom_underline">Description</h3>
      
      <div class="well guide">
        <h5 class="guide_underline">
      </div>
      
      <div class="walk_content">
        <%=raw @walk.description %>
      </div>
    </section>
    
    <% unless @walk.route.blank? %>
    <section>
      <h3 class="bottom_underline">Route</h3>
      <div class="content_padding">
        <%= raw @walk.route %>
      </div>
    </section>
    <% end %>
    

  
</section>