<% content_for :body_content_title do %>Past Events<% end%>
<% content_for :body do %>
  <ul id="sessions">
    <% @sessions.each do |session| %>
      <li>
        <%= link_to session.title, refinery.sessions_session_path(session) %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
